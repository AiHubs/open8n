{
	"$schema": "https://turborepo.org/schema.json",
	"pipeline": {
		"build": {
			"dependsOn": ["^build"]
		},
		"test": {
			"dependsOn": ["build"],
			"inputs": [
				"src/**/*.tsx",
				"src/**/*.ts",
				"test/**/*.ts"
			],
			"outputMode": "full"
		},
		"dev": {
			"cache": false,
			"outputMode": "full"
		}
	}
}
