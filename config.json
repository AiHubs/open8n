{
  "name": "Hass n8n",
  "version": "0.196.0_1",
  "slug": "hass-n8n",
  "description": "Home Assistant n8n instance with ingress",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "startup": "application",
  "boot": "auto",
  "icon": "icon.png",
  "logo": "logo.png",
  "webui": "http://[HOST]:[PORT:5678]/workflow",
  "map": ["config:rw", "ssl:rw", "addons:rw", "share:rw", "backup:rw"],
  "apparmor": true,
  "ingress": true,
  "ingress_port": 5678,
  "ingress_stream": true,
	"//ingress_entry": "workflow",
  "host_network": true,
  "//hassio_api": true,
  "//hassio_role": "manager",
  "//homeassistant_api": true,
  "auth_api": true,
  "panel_icon": "mdi:hexagon-multiple",
  "init": false,
  "ports": {
    "5678/tcp": 8099,
    "5678/tcp": 5678
  },
  "ports_description": {
    "5678/tcp": "n8n Web interface"
  },
  "options": {
    "auth": false,
    "auth_username": "",
    "auth_password": "",
    "timezone": "America/New_York",
    "protocol": "http",
    "certfile": "fullchain.pem",
    "keyfile": "privkey.pem",
    "data_path": "/data/n8n",
    "personalized": false
  },
  "schema": {
    "auth": "bool",
    "auth_username": "str?",
    "auth_password": "str?",
    "timezone": "str",
    "protocol": "str",
    "certfile": "str",
    "keyfile": "str",
    "data_path": "str",
    "personalized": "bool"
  }
}
